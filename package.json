{
  "name": "chargebee-languages",
  "version": "1.0.0",
  "description": "Easy to use method to translate language packs for chargebee.",
  "main": "TranslateChargebee.js",
  "scripts": {
    "translate": "TRANSLATE=true node TranslateChargebee.js",
    "save-reviewed": "SAVE_UNREVIEWED_TRANSLATIONS=true node TranslateChargebee.js",
    "delete-reviews": "DELETE_REVIEW_FILES=true node TranslateChargebee.js",
    "update-project-folder": "UPDATE_PROJECT_FOLDER=true node TranslateChargebee.js",
    "remove-duplicate-keys": "REMOVE_DUPLICATE_KEYS=true node TranslateChargebee.js",
    "fix-mismatched-keys": "FIX_MISMATCHED_KEYS=true node TranslateChargebee.js"
  },
  "author": "Kevin Danikowski",
  "license": "MIT",
  "dependencies": {
    "@supercharge/promise-pool": "^1.6.1",
    "axios": "^0.21.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "lodash": "^4.17.21",
    "ora": "^5.4.0",
    "prompt-confirm": "^2.0.4"
  }
}
